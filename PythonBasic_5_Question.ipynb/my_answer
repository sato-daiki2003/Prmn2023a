        "[24, 25, 31, 32, 37, 38]  \n",
        "['三重', '滋賀', '鳥取', '島根', '香川', '愛媛']\n",
        "['津', '大津', None, '松江', '高松', '松山']\n",
        "```"
      ],
      "metadata": {
        "id": "dQ2n3OMSUXh2"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "# 入力用\n",
        "list_cod=[]\n",
        "list_pre=[]\n",
        "list_cit=[]\n",
        "for i in range(6):\n",
        " list_cod.append(todohuken_data[i].get(\"コード\"))\n",
        " list_pre.append(todohuken_data[i].get(\"都道府県\"))\n",
        " list_cit.append(todohuken_data[i].get(\"県庁所在地\"))\n",
